{
  "version": 2,
  "functions": {
    "api/app.py": {
      "runtime": "@vercel/python@3.12",
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/app.py"
    },
    {
      "source": "/(.*)",
      "destination": "/api/app.py"
    }
  ]
}
